{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Grpc": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "Issuer": "Votinger.AuthServer",
    "Audience": "User",
    "Lifetime": 3600
  },
  "GrpcEndpoints": {
    "AuthServer": "https://authserver:5000",
    "PollServer": "https://pollserver:5000"
  }
}
